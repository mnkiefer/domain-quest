{
	"compilerOptions": {
	  "module": "Node16",
	  "target": "ES2022",
	  "outDir": "out",
	  "lib": [
		"ES2022",
		"DOM"
	  ],
	  "sourceMap": true,
	  "rootDir": "src",
	  "strict": true,
	  "jsx": "react",
	  "jsxFactory": "vscpp",
	  "jsxFragmentFactory": "vscppf",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": [
	  "src/**/*.ts",
	  "index.d.ts"
	],
	"exclude": [
	  "node_modules",
	  "out"
	]
  }